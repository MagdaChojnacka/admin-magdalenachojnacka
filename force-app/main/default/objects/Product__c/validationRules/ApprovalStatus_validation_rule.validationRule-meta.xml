<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ApprovalStatus_validation_rule</fullName>
    <active>true</active>
    <errorConditionFormula>ISPICKVAL( Status__c , &quot;Active&quot;)&amp;&amp;  NOT( ISPICKVAL( ApprovalStatus__c , &quot;Accepted&quot;) )</errorConditionFormula>
    <errorDisplayField>ApprovalStatus__c</errorDisplayField>
    <errorMessage>Submit status for approval</errorMessage>
</ValidationRule>
