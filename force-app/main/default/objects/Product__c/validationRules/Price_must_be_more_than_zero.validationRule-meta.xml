<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Price_must_be_more_than_zero</fullName>
    <active>true</active>
    <errorConditionFormula>Price__c    &lt;= 0</errorConditionFormula>
    <errorDisplayField>Price__c</errorDisplayField>
    <errorMessage>Price must be higher than zero, please change the price</errorMessage>
</ValidationRule>
